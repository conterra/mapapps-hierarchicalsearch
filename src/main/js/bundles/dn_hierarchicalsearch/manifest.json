{
    "name": "dn_hierarchicalsearch",
    "version": "2.1.1-SNAPSHOT",
    "title": "${bundleName}",
    "description": "${bundleDescription}",
    "main": "",
    "vendor": "con terra GmbH",
    "productName": "devnet-mapapps-hierarchical-search",
    "dependencies": {
        "apprt-vue": "^4.2.2",
        "apprt-binding": "^4.2.2",
        "apprt-vuetify": "^4.2.2",
        "agssearch": "^4.2.2",
        "map-actions": "^4.2.2"
    },
    "layout-widgets": [
        {
            "widgetRole": "hierarchicalSearchWidget",
            "sublayout": ["desktop"],
            "window": {
                "autofocus": false,
                "dockTool": "hierarchicalSearchToggleTool",
                "closable": true,
                "minimizeOnClose": true,
                "resizable": true,
                "marginBox": {
                    "w": 450,
                    "h": 170
                },
                "minSize": {
                    "w": 450,
                    "h": 170
                }
            }
        },
        {
            "widgetRole": "hierarchicalSearchWidget",
            "sublayout": ["tablet_landscape", "tablet_portrait"],
            "window": {
                "autofocus": false,
                "dockTool": "hierarchicalSearchToggleTool",
                "closable": true,
                "minimizeOnClose": true,
                "resizable": false,
                "draggable": false,
                "marginBox": {
                    "w": 450,
                    "h": 170
                }
            }
        },
        {
            "widgetRole": "hierarchicalSearchWidget",
            "sublayout": ["mobile_landscape", "mobile_portrait"],
            "window": {
                "autofocus": false,
                "dockTool": "hierarchicalSearchToggleTool",
                "minimizeOnClose": true,
                "resizable": false,
                "draggable": false,
                "marginBox": {
                    "w": "100%",
                    "h": "50%",
                    "t": 0
                },
                "windowClass": "mobileWindow"
            }
        }
    ],
    "components": [
        {
            "name": "Config",
            "impl": "./HierarchicalSearchModel",
            "propertiesConstructor": true,
            "provides": ["dn_hierarchicalsearch.HierarchicalSearchModel"],
            "properties": {
                "storeId": "",
                "fields": [],
                "mapActions": [],
                "mapActionsConfig": {}
            },
            "references": [
                {
                    "name": "store",
                    "providing": "ct.api.Store",
                    "filter": "(id={storeId})"
                },
                {
                    "name": "tool",
                    "providing": "ct.tools.Tool",
                    "filter": "(id=hierarchicalSearchToggleTool)"
                },
                {
                    "name": "logService",
                    "providing": "ct.framework.api.LogService"
                },
                {
                    "name": "actionService",
                    "providing": "map-actions.ActionService"
                }
            ]
        },
        {
            "name": "HierarchicalSearchWidgetFactory",
            "provides": [
                "dijit.Widget",
                "dn_hierarchicalsearch.HierarchicalSearchVueWidget"
            ],
            "instanceFactory": true,
            "properties": {
                "widgetRole": "hierarchicalSearchWidget"
            },
            "references": [
                {
                    "name": "hierarchicalSearchModel",
                    "providing": "dn_hierarchicalsearch.HierarchicalSearchModel"
                },
                {
                    "name": "hierarchicalSearchController",
                    "providing": "dn_hierarchicalsearch.HierarchicalSearchController"
                }
            ]
        },
        {
            "name": "HierarchicalSearchController",
            "propertiesConstructor": true,
            "provides": [
                "dn_hierarchicalsearch.HierarchicalSearchController"
            ],
            "properties": {},
            "references": [
                {
                    "name": "hierarchicalSearchModel",
                    "providing": "dn_hierarchicalsearch.HierarchicalSearchModel"
                }
            ]
        },
        {
            "name": "HierarchicalSearchToggleTool",
            "impl": "ct/tools/Tool",
            "provides": ["ct.tools.Tool"],
            "propertiesConstructor": true,
            "properties": {
                "id": "hierarchicalSearchToggleTool",
                "title": "${tool.title}",
                "tooltip": "${tool.tooltip}",
                "toolRole": "toolset",
                "iconClass": "icon-magnifier",
                "togglable": true
            }
        },
        {
            "name": "SendResultToResultUIAction",
            "impl": "./actions/SendResultToResultUIAction",
            "provides": ["map-actions.Action"],
            "references": [
                {
                    "name": "resultViewerService",
                    "providing": "result-api.ResultViewerService",
                    "cardinality": "0..1"
                },
                {
                    "name": "logService",
                    "providing": "ct.framework.api.LogService"
                }
            ]
        },
        {
            "name": "SendResultToResultCenterAction",
            "impl": "./actions/SendResultToResultCenterAction",
            "provides": ["map-actions.Action"],
            "references": [
                {
                    "name": "dataModel",
                    "providing": "resultcenter.DataModel",
                    "cardinality": "0..1"
                },
                {
                    "name": "logService",
                    "providing": "ct.framework.api.LogService"
                }
            ]
        }
    ]
}
